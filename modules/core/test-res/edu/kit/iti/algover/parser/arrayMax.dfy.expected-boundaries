
(COMPILATION_UNIT[method-}] 
 (method[method-}] max[max-max] 
  (ARGS[a-int] 
   (VAR[a->] a[a-a] 
    (array[array->] int[int-int])) 
   (VAR[n-int] n[n-n] int[int-int])) 
  (returns[returns-)] 
   (VAR[m-int] m[m-m] int[int-int])) 
  (requires[requires-0] 
   (>[n-0] n[n-n] 0[0-0])) 
  (ensures[ensures-)] 
   (label[label-:] greater[greater-greater]) 
   (forall[(-)] i[i-i] int[int-int] 
    (==>[0-]] 
     (&&[0-n] 
      (<=[0-i] 0[0-0] i[i-i]) 
      (<[i-n] i[i-i] n[n-n])) 
     (>=[a-]] 
      (ARRAY_ACCESS[a-]] a[a-a] m[m-m]) 
      (ARRAY_ACCESS[a-]] a[a-a] i[i-i]))))) 
  (ensures[ensures-)] 
   (label[label-:] witness[witness-witness]) 
   (exists[(-)] i[i-i] int[int-int] 
    (&&[0-]] 
     (&&[0-n] 
      (<=[0-i] 0[0-0] i[i-i]) 
      (<[i-n] i[i-i] n[n-n])) 
     (==[a-]] 
      (ARRAY_ACCESS[a-]] a[a-a] m[m-m]) 
      (ARRAY_ACCESS[a-]] a[a-a] i[i-i]))))) 
  (decreases[decreases-0] 0[0-0]) 
  (BLOCK[assume-}] 
   (assume[assume-;] 
    (==[n-Length] n[n-n] 
     (Length[a-Length] a[a-a]))) 
   (var[var-;] i[i-i] int[int-int] 0[0-0]) 
   (:=[m-;] m[m-m] 0[0-0]) 
   (while[label-}] 
    (label[label-:] mainLoop[mainLoop-mainLoop]) 
    (<[i-n] i[i-i] n[n-n]) 
    (invariant[invariant-n] 
     (label[label-:] inbounds[inbounds-inbounds]) 
     (&&[0-n] 
      (<=[0-i] 0[0-0] i[i-i]) 
      (<=[i-n] i[i-i] n[n-n]))) 
    (invariant[invariant-)] 
     (label[label-:] greater[greater-greater]) 
     (forall[(-)] j[j-j] int[int-int] 
      (==>[0-]] 
       (&&[0-i] 
        (<=[0-j] 0[0-0] j[j-j]) 
        (<[j-i] j[j-j] i[i-i])) 
       (>=[a-]] 
        (ARRAY_ACCESS[a-]] a[a-a] m[m-m]) 
        (ARRAY_ACCESS[a-]] a[a-a] j[j-j]))))) 
    (invariant[invariant-)] 
     (label[label-:] witness[witness-witness]) 
     (||[m-)] 
      (==[m-0] m[m-m] 0[0-0]) 
      (exists[(-)] j[j-j] int[int-int] 
       (&&[0-]] 
        (&&[0-i] 
         (<=[0-j] 0[0-0] j[j-j]) 
         (<[j-i] j[j-j] i[i-i])) 
        (==[a-]] 
         (ARRAY_ACCESS[a-]] a[a-a] m[m-m]) 
         (ARRAY_ACCESS[a-]] a[a-a] j[j-j])))))) 
    (invariant[invariant-n] 
     (label[label-:] witness_in_bounds[witness_in_bounds-witness_in_bounds]) 
     (&&[0-n] 
      (<=[0-m] 0[0-0] m[m-m]) 
      (<[m-n] m[m-m] n[n-n]))) 
    (decreases[decreases-i] 
     (-[n-i] n[n-n] i[i-i])) 
    (BLOCK[{-}] 
     (if[if-}] 
      (>[(-)] 
       (ARRAY_ACCESS[a-]] a[a-a] i[i-i]) 
       (ARRAY_ACCESS[a-]] a[a-a] m[m-m])) 
      (BLOCK[{-}] 
       (:=[m-;] m[m-m] i[i-i]))) 
     (:=[i-;] i[i-i] 
      (+[i-1] i[i-i] 1[1-1])))))))


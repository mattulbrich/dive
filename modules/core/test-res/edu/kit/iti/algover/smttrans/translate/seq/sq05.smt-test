s1: seq<int>
s2: seq<int>
---
|-
let s1 := $seq_cons<int>(4,$seq_cons<int>(3,$seq_cons<int>(2,$seq_cons<int>(1,$seq_empty<int>)))) :: 
let s2 := $seq_cons<int>(8,$seq_cons<int>(7,$seq_cons<int>(6,$seq_cons<int>(5,$seq_empty<int>)))) ::
forall i:int :: $imp($and($le(0,i),$lt(i,4)),$seq_get<int>(s1,i) == $minus($seq_get<int>(s2,i),4))